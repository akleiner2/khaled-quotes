{
  "manifest_version": 2,

  "name": "Khaled Quotes",
  "description": "Get the keys to success right in your browser",
  "version": "1.0",
  "commands": {
    "copy-quote": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Copies random quote to clipboard"
    }
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}